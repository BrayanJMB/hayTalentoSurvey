                        <div>
                            <div class="addQuestion mt-2">
                                <div class="question-list ">
                                            @if (categori.Preguntas != null)
                                            {
                                        <h3 class="titulo-dimension">@categori.NombreCategoria</h3>
                                        <p class="descripcion-dimension">@categori.Description</p>
